<!DOCTYPE html>
<html>
<head>
  <title>Membr</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "jquery.tinycarousel.min.js" %>
  <%= javascript_include_tag "jquery.customSelect.min" %>
  <%= csrf_meta_tags %>

  <%= yield :head %>
</head>
<body class="static">

<div class="main-header">
  <div class="container static-cont">
    <%= link_to root_url do %>
      <%= image_tag "logo.png" %>
    <% end %>

    <div class="links">
      <a class="invite-link">Request an Invite</a>
    </div>
  </div>
</div>

<%= yield %>

<div class="main-footer">
  <div class="container static-cont">
    <%= link_to "ABOUT", terms_path %>
    <%= link_to "TERMS", terms_path %>
    <%= link_to "FAQ", faq_path %>
  </div>
</div>

</body>
</html>
